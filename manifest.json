{
  "manifest_version": 2,

  "name": "Map Switcher",
  "description": "Switch between different maps",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "mapswitcher.html",
    "default_title": "Map Switcher"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "background": {
    "scripts": ["jquery-2.2.4.min.js"]
  },
  "content_scripts": [{
    "matches": [
      "*://www.google.*/maps/*",
      "*://www.bing.com/maps/*",
      "*://www.bing.com/mapspreview/*",
      "*://www.geocaching.com/map/*"
    ],
    "js": [
      "dataExtractor.js"
    ]
  }]
}
